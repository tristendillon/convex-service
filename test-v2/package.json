{
  "name": "convex-service-test-v2",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "convex dev",
    "predev": "convex dev --until-success && convex dashboard",
    "logs": "convex logs",
    "lint": "tsc -p convex && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "rm -rf node_modules/.vite && vitest",
    "test:once": "rm -rf node_modules/.vite && vitest run --environment=node",
    "test:debug": "rm -rf node_modules/.vite && vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "rm -rf node_modules/.vite && vitest run --coverage --coverage.reporter=text"
  },
  "dependencies": {
    "@lunarhue/convex-service": "file:../",
    "convex": "^1.23.0"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/browser": "^3.2.4",
    "convex-test": "^0.0.38",
    "eslint": "^8.49.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide"
    ]
  }
}
