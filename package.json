{
  "name": "@lunarhue/convex-service",
  "version": "0.1.0",
  "description": "A TypeScript package that wraps Convex schemas with Zod integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./v2": "./dist/v2/index.js",
    "./v2/server": "./dist/v2/server/index.js",
    "./v2/server/zod": "./dist/v2/server/zod/index.js",
    "./v2/server/hooks": "./dist/v2/server/hooks/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "convex",
    "schema",
    "zod",
    "typescript",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "convex": "^1.25.4",
    "convex-helpers": "^0.1.104",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^1.2.0",
    "convex-test": "^0.0.38",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
