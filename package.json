{
  "name": "convex-sql",
  "version": "0.1.0",
  "description": "A TypeScript package that wraps Convex schemas with Zod integration",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
